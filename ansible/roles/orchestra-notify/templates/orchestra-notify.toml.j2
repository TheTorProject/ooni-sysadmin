[core]
environment = "{{ orchestra_environment }}"
log-level = "{{ orchestra_log_level }}"
# worker-num: 0 # defaults to CPU number
# queue-size: 2048

[auth]
jwt-secret = "{{ orchestra_auth_jwt_secret }}"

[api]
port = {{ orchestra_api_port }}
address = "{{ orchestra_api_address }}"

[database]
url = "{{ orchestra_database_url }}"
active-probes-table = "active_probes"
probe-updates-table = "probe_updates"

[fcm]
server-key = "{{ orchestra_fcm_server_key }}"
max-retries = 5

[apn]
max-retries = 5
key-path = "{{ orchestra_apn_key_path }}"
key-password = "{{ orchestra_apn_key_password }}"
production = {{ orchestra_apn_production }}
